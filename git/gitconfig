[color]
	ui = true
[user]
	name = Vadim Brodsky
	email = vadim.brodsky@gmail.com
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[core]
	editor = nvim
	autocrlf = input
	excludesfile = /Users/vbrodsky/.gitignore
[alias]
  a = add
  aa = add -A
  b = branch
  bv = branch -v --sort=committerdate
  co = checkout
  cob = checkout -b
  d = diff
  s = status
